{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-camera",
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir el acceso a la cámara para medir los signos vitales.",
          "microphonePermission": "Permitir el acceso al micrófono para grabar audio si es necesario."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
